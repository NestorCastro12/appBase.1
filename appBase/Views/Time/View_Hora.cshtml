@model appBase.Models.Lista_Horas
@{
    ViewBag.Title = "View_Hora";
}


<div class="">
    <div class="page-title">
        <div class="title_left">
        </div>


    <div class="clearfix"></div>

    <div class="row">
        <div class="col-md-12 col-sm-12 ">
            <div class="x_panel">
                <div class="x_title">
                    <h2>Tabla reportes <small></small></h2>
                    <ul class="nav navbar-right panel_toolbox">
                        <li>
                            <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                        </li>
                        <li><a class="glyphicon glyphicon-repeat" href="/Time/View_Hora"></a></li>
                        <li>
                            <a class="close-link"><i class="fa fa-close"></i></a>
                        </li>
                    </ul>
                    <div class="clearfix"></div>
                </div>




                <div class="x_content">
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="card-box table-responsive">
                                <p class="text-muted font-13 m-b-30">
                                </p>
                                <table id="datatable" class="table table-striped table-bordered" style="width:100%">
                                    <thead>
                                        <tr>
                                            <th>Id</th>
                                            <th>Hora</th>
                                            <th>Eliminar</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var InLista in Model.List_Horas)
                                        {
                                        <tr>
                                            <th scope="row">@InLista.ID</th>
                                            <td>@InLista.Hora</td>
                                            <td>

                                                <a class="btn btn-round btn-danger" href="~/Time/Delete/@InLista.ID"> Borrar</a>

                                            </td>
                                        </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>